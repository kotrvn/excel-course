{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "Atomics":"readonly",
        "SharedArrayBuffer":"readonly"
    },
    "rules": {
        "semi": "off",
        "require-jsdoc": "off",
        "eol-last":"off",
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "quotes": "off",
        "no-trailing-spaces": "off",
        "arrow-parens": "off",
        "operator-linebreak": "off"
    },
    "extends": ["eslint:recommended", "google"]
}